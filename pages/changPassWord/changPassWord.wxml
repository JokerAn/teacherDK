<view id='myChangePassWord'>
<view class='inputs'>
  <view><text>旧密码</text><input type='password' bindinput="oldPwdF" value='{{oldPwd}}'  placeholder='请输入旧密码' placeholder-class='placehoderClass'></input></view>
  <view><text>新密码</text><input type='password' bindinput="newPwdF" value='{{newPwd}}'  placeholder='请输入新密码' placeholder-class='placehoderClass'></input></view>
  <view><text>确认密码</text><input type='password' bindinput="newPwdF02" value='{{newPwd02}}'  placeholder='请再次输入新密码' placeholder-class='placehoderClass'></input></view>
</view>
  
  <view class='btnOk' bindtap="btnAjax">确认修改密码</view>
</view>
