#login {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 100rpx 0 0 0;
  box-sizing: border-box;
}

.login_phcolor {
  font-size: 26rpx;
  color: #dfdfdf;
}
#login>view>view>text{
  font-size: 14px;
}
#login input {
  width: 400rpx;
  height: 77rpx;
  border-bottom: 1rpx solid #dfdfdf;
  padding-left: 100rpx;
  padding-right: 50rpx;
  display:inline-block;
  vertical-align:bottom;
  font-size: 14px;

}

.login_btn {
  display: inline-block;
  width: 553rpx;
  height: 80rpx;
  line-height: 80rpx;
  background: #f63e5a;
  font-size: 30rpx;
  color: #fff;
  margin-bottom: 22rpx;
  border-radius: 100rpx;
  text-align: center;
}
.loginBtnBox{
  margin:50rpx 0 0 0;
}
.canotLogin{
  font-size: 12px;
  color:blue;
}
.canotLogin text{
  padding:0 10rpx;
  margin-left:10rpx;
}
.canotLogin navigator{
  display: inline
}
.emailLogin{text-align: center}


.login_group{
  position: relative;
  margin-bottom: 39rpx;
}
.login_input{
  width: 260rpx;
  height: 77rpx;
  border-bottom: 1rpx solid #dfdfdf;
  padding-left: 88rpx;
  padding-right: 205rpx;
}
.login_group>image,.login_group>.login_code{
  position: absolute;
}
.login_man_ico{
  width: 44rpx;
  height: 46rpx;
  left: 11rpx;
  top: 15rpx;
}
.login_phone_ico{
  width: 30rpx;
  height: 45rpx;
  left: 18rpx;
  top: 16rpx;
}
.login_phcolor{
  font-size: 26rpx;
  color: #dfdfdf
}